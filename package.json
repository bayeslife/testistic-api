{
  "name": "testistic-api",
  "version": "1.1.1",
  "description": "API services to support test statistics presentation",
  "main": "index.js",
  "scripts": {
    "docker": "docker build -t gcr.io/testistic-216703/testistic-api .",
    "build": "rollup -c",
    "start:watch": "nodemon -w node_modules/testistic-base -w node_modules/testistic-repository  -w src -w -x 'npm run test && npm run lint && DEBUG=kafka-node:*,kafka-client,testistic-api,testistic-repository node -r reify src/index.js'",
    "start": "DEBUG=kafka-node:*,kafka-client,testistic-api,testistic-repository,testistic-model node -r reify  -r source-map-support/register src/index.js",
    "lint": "./node_modules/.bin/eslint --fix src/**/*.js",
    "test": "mocha -r reify -r source-map-support/register 'src/**/*.spec.js'"
  },
  "author": "phil@xceptionale.com",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "reify": "^0.16.4",
    "testistic-repository": "^1.1.5"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "opn-cli": "^3.1.0",
    "rollup": "^0.61.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "source-map-support": "^0.5.9"
  }
}
